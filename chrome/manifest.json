{
  "name": "Greasefire",
  "version": "0.1",
  "description": "Automatically find user scripts from Userscripts.org",
  "permissions": [
    "tabs",
    "unlimited_storage",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background_page": "background.html",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "Userscripts Available",
    "popup": "picker.html"
  }
}
